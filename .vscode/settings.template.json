{
  "rslint.binPath": "./packages/rslint/bin/rslint",
  "rslint.enable": true,
  "[json]": {
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "files.associations": {
    "rslint.json": "jsonc"
  },
  "json.schemas": [
    {
      "fileMatch": [
        "rslint.json",
        "rslint.jsonc"
      ],
      "url": "./rslint-schema.json"
    }
  ],
  "cSpell.words": [
    "rslint"
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
