{
  "name": "rsrun",
  "version": "0.0.2",
  "description": "TypeScript Executer powered by TSGO",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rslint"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "require": "./dist/register.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "rsrun": "./bin/rsrun"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "node --test tests/e2e.test.mjs"
  },
  "dependencies": {
    "@rslint/tsgo": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "24.0.14",
    "typescript": "5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
