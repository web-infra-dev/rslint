{
  "name": "@rslint/core",
  "version": "0.0.1",
  "exports": {
    ".": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build:bin": "go build -o bin/rslint ../../cmd/tsgolint",
    "build:js": "tsc -b tsconfig.build.json --force",
    "dev": "tsc -b tsconfig.build.json --force --watch",
    "build": "pnpm build:bin && pnpm build:js",
    "test": "node --test 'tests/**.test.mjs'",
    "test:update": "node --test --test-update-snapshots 'tests/**.test.mjs'"
  },
  "files": [
    "bin/"
  ],
  "bin": {
    "rslint": "./bin/rslint"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "description": "rslint cli",
  "publisher": "rslint",
  "keywords": [
    "rslint",
    "linter",
    "typescript",
    "go"
  ],
  "devDependencies": {
    "@types/node": "24.0.14",
    "typescript": "5.8.3"
  }
}
